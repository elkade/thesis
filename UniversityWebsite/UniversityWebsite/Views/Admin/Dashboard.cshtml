<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"> 
		<meta name="viewport" content="width=device-width, initial-scale=1.0"> 
		<title>Dashboard</title>
		@Styles.Render("~/Content/admin/css")
        <script src="/Scripts/admin/modernizr.custom.js"></script>
	</head>
	<body>
		<div class="container">	
			<!-- Codrops top bar -->
			<div class="codrops-top clearfix">
				<a href="http://1.bp.blogspot.com/-RYP0Df6wgyE/T9dTpMIEzFI/AAAAAAAAC50/g-ZaYfumQ90/s1600/Sasha-Grey+04.jpg"><strong>Dla dzieci</strong></a>
				<span class="right"><a href="@Url.Action("Index", "Home")"><strong>Powrot do strony</strong></a></span>
			</div><!--/ Codrops top bar -->
			<header class="clearfix">
				<h1>Panel admina<span>Zarzadzanie systemem</span></h1>	
			</header>
			<div class="main">

				<ul id="rb-grid" class="rb-grid clearfix">
					<li class="icon-clima-2 rb-span-2">
						<h3>Użytkownicy</h3><span class="rb-temp">Zarzadzaj użytkownikami systemu</span>
						<div class="rb-overlay" style="background: blue;">
							<span class="rb-close">close</span>
						</div>
					</li>
					<li class="icon-clima-1">
						<h3>Przedmioty</h3><span class="rb-temp">Zarządzaj i dodawaj nowe przedmioty</span>
						<div class="rb-overlay" style="background: blue;">
							<span class="rb-close">close</span>
						</div>
					</li>
					<li class="icon-clima-4">
						<h3>Menu nawigacyjne</h3><span class="rb-temp">Dostosuj górne menu</span>
						<div class="rb-overlay" style="background: blue;">
							<span class="rb-close">close</span>
						</div>
					</li>
                    <li class="icon-clima-1 rb-span-4">
						<h3>Strony</h3><span class="rb-temp">Modyfikuj i dodawaj nowe strony</span>
						<div class="rb-overlay" style="background: blue;">
							<span class="rb-close">close</span>
						</div>
					</li>
				</ul>
				
			</div>
		</div><!-- /container -->
		<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
         @Scripts.Render("~/bundles/admin")
		<script>
		    $(function () {

		        Boxgrid.init();


		    });
		</script>
	</body>
</html>




